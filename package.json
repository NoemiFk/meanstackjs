{
  "name": "meanstackjs",
  "version": "6.0.0-alpha.1",
  "description": "meanstack",
  "scripts": {
    "start": "npm run build && node ./run.js",
    "start:dev": "./node_modules/.bin/concurrently \"./node_modules/.bin/ng serve\" \"./node_modules/.bin/nodemon ./run.js\"",
    "start:production": "npm run build:production && node ./run.js",
    "test": "npm run mocha && npm run lint",
    "mocha": "node ./tests/mocha.js mocha --exit",
    "ngtest": "./node_modules/.bin/ng test --watch=false ",
    "lint": "npm run lint:fix:frontend && npm run lint:fix:backend",
    "lint:fix": "npm run lint:fix:frontend && npm run lint:fix:backend",
    "lint:fix:frontend": "npm run lint:fix:frontend:first && npm run lint:fix:frontend:second && npm run lint:fix:frontend:third ",
    "lint:fix:frontend:first": "node_modules/.bin/tslint -p tsconfig.json -c tslint.json --exclude client/*.spec.ts client/*.ts --fix",
    "lint:fix:frontend:second": "node_modules/.bin/tslint -p tsconfig.json -c tslint.json --exclude client/**/*.spec.ts  client/**/*.ts --fix",
    "lint:fix:frontend:third": "node_modules/.bin/tslint -p tsconfig.json -c tslint.json --exclude client/**/**/*.spec.ts  client/**/**/*.ts --fix",
    "lint:fix:backend": "node_modules/.bin/standard --fix",
    "postinstall": "node scripts/postinstall.js",
    "build": "./node_modules/.bin/ng build",
    "build:production": "./node_modules/.bin/ng build --prod",
    "e2e": "./node_modules/.bin/ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://GreenPioneer@github.com/greenpioneersolutions/menstackjs.git"
  },
  "keywords": [
    "menstackjs"
  ],
  "author": "GPS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greenpioneersolutions/menstackjs/issues"
  },
  "homepage": "https://github.com/greenpioneersolutions/menstackjs#readme",
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ng-bootstrap/ng-bootstrap": "^3.2.0",
    "auto-parse": "1.5.1",
    "bootstrap": "^4.1.3",
    "compression": "1.7.3",
    "connect-mongo": "2.0.1",
    "cookie-parser": "1.4.3",
    "core-js": "^2.5.4",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "express-validator": "5.3.0",
    "font-awesome": "^4.7.0",
    "glob": "7.1.3",
    "helmet": "3.13.0",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.10",
    "method-override": "3.0.0",
    "moment": "^2.22.2",
    "mongoose": "5.2.12",
    "morgan": "1.9.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "run-auto": "2.0.3",
    "rxjs": "^6.0.0",
    "sendmail": "1.4.1",
    "serve-favicon": "2.5.0",
    "socket.io": "2.1.1",
    "spdy": "3.4.7",
    "winston": "3.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.5",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "chai": "4.1.2",
    "codelyzer": "~4.2.1",
    "concurrently": "^4.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "5.2.0",
    "nodemon": "1.18.4",
    "protractor": "~5.4.0",
    "shelljs": "0.8.2",
    "standard": "12.0.1",
    "supertest": "3.1.0",
    "ts-node": "~5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "~2.7.2"
  },
  "standard": {
    "globals": [
      "angular",
      "toastr",
      "moment",
      "self",
      "_",
      "describe",
      "it",
      "__dirname",
      "localStorage",
      "io",
      "expect",
      "inject",
      "beforeEach",
      "before",
      "afterEach",
      "after",
      "screen",
      "jasmine"
    ],
    "ignore": [
      "logs/",
      "database/",
      "downloads/",
      "commands/",
      "backups/",
      "tools/",
      "test/",
      "/dist",
      "*.spec.js"
    ]
  }
}
